<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="test_manager_tree_view" model="ir.ui.view">
        <field name="name">test.manager.tree.view</field>
        <field name="model">test.manager</field>
        <field name="priority" eval="25"/>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>
    <record id="test_manager_form_view" model="ir.ui.view">
        <field name="name">test.manager.form.view</field>
        <field name="model">test.manager</field>
        <field name="priority" eval="25"/>
        <field name="arch" type="xml">
            <form>
                <field name="name" />
            </form>
        </field>
    </record>

    <record id="test_manager_action" model="ir.actions.act_window">
        <field name="name">Test manager</field>
        <field name="res_model">test.manager</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>
